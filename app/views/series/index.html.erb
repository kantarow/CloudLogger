<p id="notice"><%= notice %></p>

<h1>Series</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @series.each do |series| %>
      <tr>
        <td><%= series.name %></td>
        <td><%= link_to 'Show', data_logger_series_path(@data_logger, series) %></td>
        <td><%= link_to 'Edit', edit_data_logger_series_path(@data_logger, series) %></td>
        <td><%= link_to 'Destroy', data_logger_series_path(@data_logger, series), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Series', new_data_logger_series_path %>
