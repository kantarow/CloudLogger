<h1>Data Loggers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_loggers.each do |data_logger| %>
      <tr>
        <td><%= data_logger.name %></td>
        <td><%= link_to 'Show', data_logger %></td>
        <td><%= link_to 'Edit', edit_data_logger_path(data_logger) %></td>
        <td><%= link_to 'Destroy', data_logger, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Logger', new_data_logger_path %>
